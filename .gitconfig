[init]
	defaultBranch = master

[user]
	email = nehls@email.arizona.edu
	name = JohnNehls

[url "git@github.com:"]
	insteadOf = https://github.com/

[core]
	editor = emacs

[merge]
	conflictstyle = diff3
	tool = smerge

[diff]
	tool = ediff

[difftool "ediff"]
	cmd = emacs --eval \"(ediff-files \\\"$LOCAL\\\" \\\"$REMOTE\\\")\"
	
[difftool "ediffclient"]
        cmd = emacsclient --eval \"(ediff-files \\\"$LOCAL\\\" \\\"$REMOTE\\\")\"

